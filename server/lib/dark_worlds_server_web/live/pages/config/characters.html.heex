<.header>
    Edit Character Config
</.header>
  
<.simple_form :let={f} for={@config} action={#}>
    <%= for key <- @config |> Map.keys() |> Enum.map(&String.to_existing_atom/1) do %>
        <.input field={f[key]} type="text" label={key} />
    <% end %>

    <:actions>
        <.button phx-click="save">Save</.button>
    </:actions>
</.simple_form>



<.back navigate={~p"/config"}>Back to config</.back>
